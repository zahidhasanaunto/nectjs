{
  "name": "nectjs",
  "version": "1.0.4",
  "description": "Next generation node.js framework",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zahidhasanaunto/nectjs.git"
  },
  "keywords": [
    "nectjs",
    "nodejs",
    "typescript",
    "koa",
    "typeorm",
    "api"
  ],
  "author": "Zahid Hasan",
  "license": "GPL3",
  "bugs": {
    "url": "https://github.com/zahidhasanaunto/nectjs/issues"
  },
  "homepage": "https://github.com/zahidhasanaunto/nectjs#readme",
  "bin": {
    "nectjs": "./dist/app.js"
  },
  "main": "./dist/app.js",
  "types": "./dist/app.d.ts",
  "scripts": {
    "dev": "npm run build && npm run watch-debug",
    "build": "npm run build-ts && npm run tslint",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "start": "npm run build && npm run serve",
    "serve": "node dist/app.js",
    "watch-node": "nodemon dist/app.js",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "serve-debug": "nodemon --inspect dist/app.js"
  },
  "dependencies": {
    "class-validator": "^0.11.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/concurrently": "^4.1.0",
    "@types/eslint": "^6.1.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.1.1",
    "@types/shelljs": "^0.8.6",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "boxen": "^4.2.0",
    "chai": "^4.2.0",
    "chalk": "^3.0.0",
    "commander": "^4.0.1",
    "concurrently": "^5.0.2",
    "eslint": "^6.8.0",
    "fs-extra": "^8.1.0",
    "mocha": "^6.2.2",
    "nodemon": "^2.0.2",
    "shelljs": "^0.8.3",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-language-service": "^0.9.9",
    "typescript": "^3.7.4",
    "write": "^2.0.0",
    "yargs": "^15.0.2"
  }
}